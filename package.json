  },
  "bin": {
    "coralcss": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && node scripts/build-css.js",
    "build:css": "node scripts/build-css.js",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "benchmark": "npx tsx benchmarks/compare.ts",
    "benchmark:full": "node --expose-gc --loader tsx benchmarks/compare.ts 5000",
    "benchmark:json": "npx tsx benchmarks/compare.ts 1000 --json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:coverage"
  },
  "keywords": [
    "css",
    "css-framework",
    "utility-css",
    "atomic-css",
    "tailwind-alternative",
    "css-utilities",
    "headless-ui",
    "headless-components",
    "design-system",
    "theming",
    "dark-mode",
    "responsive",
    "container-queries",
    "anchor-positioning",
    "modern-css",
    "scroll-driven-animations",
    "view-transitions",
    "css-nesting",
    "logical-properties",
    "zero-dependency"
  ],
